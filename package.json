{
  "name": "rockpool-app",
  "version": "1.0.0",
  "description": "An aggregator for community blogs, making subscriptions available via a Twitter bot, Mastodon bot, RSS, email, Pocket, or OPML file.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "NODE_ENV=development DEBUG=Rockpool node app.js",
    "test": "NODE_ENV=test mocha --exit",
    "migrate": "node scripts/migrator.js",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "rss",
    "blogs",
    "twitter",
    "mastodon",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hughrun/rockpool.git"
  },
  "author": "Hugh Rundle",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@hughrun/feedfinder": "^1.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.4",
    "emailjs": "^2.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "feedparser-promised": "^2.0.1",
    "mastodon": "^1.2.2",
    "moment": "^2.24.0",
    "mongodb": "^3.2.6",
    "passwordless": "^1.1.3",
    "passwordless-mongostore-bcryptjs": "^1.1.0",
    "twit": "^2.2.11",
    "util": "^0.12.0",
    "whiskers": "^0.4.0"
  },
  "devDependencies": {
    "clipboardy": "^2.1.0",
    "debug": "^4.1.1",
    "mocha": "^6.2.0",
    "nock": "^10.0.6",
    "supertest": "^4.0.2"
  }
}
